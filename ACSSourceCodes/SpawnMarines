		Spawn("Marine_Saw", GetActorX(0)-32, GetActorY(0)-32, GetActorZ(0), 0, 0);
		Spawn("Marine_Saw", GetActorX(0)-32, GetActorY(0)+32, GetActorZ(0), 0, 0);
		Spawn("Marine_Saw", GetActorX(0)+32, GetActorY(0), GetActorZ(0), 0, 0);
