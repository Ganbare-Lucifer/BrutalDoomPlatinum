		Spawn("Marine_Saw", GetActorX(0)-32, GetActorY(0)+32, GetActorZ(0), 0, 0);
		Spawn("Marine_Saw", GetActorX(0)+32, GetActorY(0), GetActorZ(0), 0, 0);
		Spawn("Marine_Saw", GetActorX(0)+32, GetActorY(0)-32, GetActorZ(0), 0, 0);
		break;

		Case 6:
		Spawn("Marine_Saw", GetActorX(0)-32, GetActorY(0), GetActorZ(0), 0, 0);
		Spawn("Marine_Saw", GetActorX(0)-32, GetActorY(0)-32, GetActorZ(0), 0, 0);
		Spawn("Marine_Saw", GetActorX(0)-32, GetActorY(0)+32, GetActorZ(0), 0, 0);
		Spawn("Marine_Saw", GetActorX(0)+32, GetActorY(0), GetActorZ(0), 0, 0);
		Spawn("Marine_Saw", GetActorX(0)+32, GetActorY(0)-32, GetActorZ(0), 0, 0);
		Spawn("Marine_Saw", GetActorX(0)+32, GetActorY(0)+32, GetActorZ(0), 0, 0);
		break;

		Case 7:
		Spawn("Marine_Saw", GetActorX(0)-32, GetActorY(0), GetActorZ(0), 0, 0);
		Spawn("Marine_Saw", GetActorX(0)-32, GetActorY(0)-32, GetActorZ(0), 0, 0);
		Spawn("Marine_Saw", GetActorX(0)-32, GetActorY(0)+32, GetActorZ(0), 0, 0);
		Spawn("Marine_Saw", GetActorX(0)+32, GetActorY(0), GetActorZ(0), 0, 0);
		Spawn("Marine_Saw", GetActorX(0)+32, GetActorY(0)-32, GetActorZ(0), 0, 0);
		Spawn("Marine_Saw", GetActorX(0)+32, GetActorY(0)+32, GetActorZ(0), 0, 0);
		Spawn("Marine_Saw", GetActorX(0), GetActorY(0)-32, GetActorZ(0), 0, 0);
		break;

		Case 8:
		Spawn("Marine_Saw", GetActorX(0)-32, GetActorY(0), GetActorZ(0), 0, 0);
		Spawn("Marine_Saw", GetActorX(0)-32, GetActorY(0)-32, GetActorZ(0), 0, 0);
		Spawn("Marine_Saw", GetActorX(0)-32, GetActorY(0)+32, GetActorZ(0), 0, 0);
		Spawn("Marine_Saw", GetActorX(0)+32, GetActorY(0), GetActorZ(0), 0, 0);
		Spawn("Marine_Saw", GetActorX(0)+32, GetActorY(0)-32, GetActorZ(0), 0, 0);
		Spawn("Marine_Saw", GetActorX(0)+32, GetActorY(0)+32, GetActorZ(0), 0, 0);
		Spawn("Marine_Saw", GetActorX(0), GetActorY(0)-32, GetActorZ(0), 0, 0);
		Spawn("Marine_Saw", GetActorX(0), GetActorY(0)+32, GetActorZ(0), 0, 0);
		break;

		Default:
		 If (NumberOfAlliesSaw < 0)
        {
            NumberOfAlliesSaw = 0;
        }
        Else
        {
		Spawn("Marine_Saw", GetActorX(0)-32, GetActorY(0), GetActorZ(0), 0, 0);
		Spawn("Marine_Saw", GetActorX(0)-32, GetActorY(0)-32, GetActorZ(0), 0, 0);
		Spawn("Marine_Saw", GetActorX(0)-32, GetActorY(0)+32, GetActorZ(0), 0, 0);
		Spawn("Marine_Saw", GetActorX(0)+32, GetActorY(0), GetActorZ(0), 0, 0);
		Spawn("Marine_Saw", GetActorX(0)+32, GetActorY(0)-32, GetActorZ(0), 0, 0);
		Spawn("Marine_Saw", GetActorX(0)+32, GetActorY(0)+32, GetActorZ(0), 0, 0);
		Spawn("Marine_Saw", GetActorX(0), GetActorY(0)-32, GetActorZ(0), 0, 0);
		Spawn("Marine_Saw", GetActorX(0), GetActorY(0)+32, GetActorZ(0), 0, 0);
		}
