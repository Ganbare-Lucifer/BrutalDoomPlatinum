#library "SpawnMarines"
#include "zcommon.acs"

Int MarineSpawnCounter = 0;
World Int 1: NumberOfAlliesFist;
World Int 2: NumberOfAlliesSaw;
World Int 3: NumberOfAlliesSMG;
World Int 4: NumberOfAlliesShotgun;
World Int 5: NumberOfAlliesSSG;
World Int 6: NumberOfAllies;
World Int 7: NumberOfAlliesMinigun;
World Int 8: NumberOfAlliesRocket;
World Int 9: NumberOfAlliesGrenade;
World Int 10: NumberOfAlliesPlasmagun;
World Int 11: NumberOfAlliesRailgun;
World Int 12: NumberOfAlliesBFG;
World Int 13: NumberOfAlliesBFG10K;
World Int 14: NumberOfAlliesHML;
World Int 15: NumberOfAlliesFlame;

World Int 16: BlackNumberOfAlliesFist;
World Int 17: BlackNumberOfAlliesSaw;
World Int 18: BlackNumberOfAlliesSMG;
World Int 19: BlackNumberOfAlliesShotgun;
World Int 20: BlackNumberOfAlliesSSG;
World Int 21: BlackNumberOfAllies;
World Int 22: BlackNumberOfAlliesMinigun;
World Int 23: BlackNumberOfAlliesRocket;
World Int 24: BlackNumberOfAlliesGrenade;
World Int 25: BlackNumberOfAlliesPlasmagun;
World Int 26: BlackNumberOfAlliesRailgun;
World Int 27: BlackNumberOfAlliesBFG;
World Int 28: BlackNumberOfAlliesBFG10K;
World Int 29: BlackNumberOfAlliesHML;
World Int 30: BlackNumberOfAlliesFlame;

//Bloodwolf333's stuff
SCRIPT "CheckForSpawningMarines" ENTER
{
if ((MarineSpawnCounter == 0) && (PlayerCount() >= 1))
	{
		ACS_NamedExecute("BDSpawnMarines",0);
	}
	else
	{
		NumberOfAlliesFist = 0;
		NumberOfAlliesSaw = 0;
		NumberOfAlliesSMG = 0;
		NumberOfAlliesShotgun = 0;
		NumberOfAlliesSSG = 0;
		NumberOfAllies = 0;
		NumberOfAlliesMinigun = 0;
		NumberOfAlliesRocket = 0;
		NumberOfAlliesGrenade = 0;
		NumberOfAlliesPlasmagun = 0;
		NumberOfAlliesRailgun = 0;
		NumberOfAlliesBFG = 0;
		NumberOfAlliesBFG10K = 0;
		NumberOfAlliesHML = 0;
		NumberOfAlliesFlame = 0;
		
		BlackNumberOfAlliesFist = 0;
		BlackNumberOfAlliesSaw = 0;
		BlackNumberOfAlliesSMG = 0;
		BlackNumberOfAlliesShotgun = 0;
		BlackNumberOfAlliesSSG = 0;
		BlackNumberOfAllies = 0;
		BlackNumberOfAlliesMinigun = 0;
		BlackNumberOfAlliesRocket = 0;
		BlackNumberOfAlliesGrenade = 0;
		BlackNumberOfAlliesPlasmagun = 0;
		BlackNumberOfAlliesRailgun = 0;
		BlackNumberOfAlliesBFG = 0;
		BlackNumberOfAlliesBFG10K = 0;
		BlackNumberOfAlliesHML = 0;
		BlackNumberOfAlliesFlame = 0;
	}
}


SCRIPT "BDSpawnMarines" (void)//ENTER
{
	//Fist Marines
	Switch (NumberOfAlliesFist)
	{
		Case 0:
		break;

		Case 1:
		Spawn("Marine_Fist", GetActorX(0)-32, GetActorY(0), GetActorZ(0), 0, 0);
		break;

		Case 2:
		Spawn("Marine_Fist", GetActorX(0)-32, GetActorY(0), GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0)-32, GetActorY(0)-32, GetActorZ(0), 0, 0);
		break;

		Case 3:
		Spawn("Marine_Fist", GetActorX(0)-32, GetActorY(0), GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0)-32, GetActorY(0)-32, GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0)-32, GetActorY(0)+32, GetActorZ(0), 0, 0);
		break;

		Case 4:
		Spawn("Marine_Fist", GetActorX(0)-32, GetActorY(0), GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0)-32, GetActorY(0)-32, GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0)-32, GetActorY(0)+32, GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0)+32, GetActorY(0), GetActorZ(0), 0, 0);
		break;

		Case 5:
		Spawn("Marine_Fist", GetActorX(0)-32, GetActorY(0), GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0)-32, GetActorY(0)-32, GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0)-32, GetActorY(0)+32, GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0)+32, GetActorY(0), GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0)+32, GetActorY(0)-32, GetActorZ(0), 0, 0);
		break;

		Case 6:
		Spawn("Marine_Fist", GetActorX(0)-32, GetActorY(0), GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0)-32, GetActorY(0)-32, GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0)-32, GetActorY(0)+32, GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0)+32, GetActorY(0), GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0)+32, GetActorY(0)-32, GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0)+32, GetActorY(0)+32, GetActorZ(0), 0, 0);
		break;

		Case 7:
		Spawn("Marine_Fist", GetActorX(0)-32, GetActorY(0), GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0)-32, GetActorY(0)-32, GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0)-32, GetActorY(0)+32, GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0)+32, GetActorY(0), GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0)+32, GetActorY(0)-32, GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0)+32, GetActorY(0)+32, GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0), GetActorY(0)-32, GetActorZ(0), 0, 0);
		break;

		Case 8:
		Spawn("Marine_Fist", GetActorX(0)-32, GetActorY(0), GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0)-32, GetActorY(0)-32, GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0)-32, GetActorY(0)+32, GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0)+32, GetActorY(0), GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0)+32, GetActorY(0)-32, GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0)+32, GetActorY(0)+32, GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0), GetActorY(0)-32, GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0), GetActorY(0)+32, GetActorZ(0), 0, 0);
		break;

		Default:
		 If (NumberOfAlliesFist < 0)
        {
            NumberOfAlliesFist = 0;
        }
        Else
        {
		Spawn("Marine_Fist", GetActorX(0)-32, GetActorY(0), GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0)-32, GetActorY(0)-32, GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0)-32, GetActorY(0)+32, GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0)+32, GetActorY(0), GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0)+32, GetActorY(0)-32, GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0)+32, GetActorY(0)+32, GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0), GetActorY(0)-32, GetActorZ(0), 0, 0);
		Spawn("Marine_Fist", GetActorX(0), GetActorY(0)+32, GetActorZ(0), 0, 0);
		}
		break;
	}

	//Saw Marines
	Switch (NumberOfAlliesSaw)
	{
		Case 0:
		break;

		Case 1:
		Spawn("Marine_Saw", GetActorX(0)-32, GetActorY(0), GetActorZ(0), 0, 0);
		break;

		Case 2:
